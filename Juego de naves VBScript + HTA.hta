     Sub Document_OnKeyDown()
        Select case Window.Event.KeyCode
        case 13:Call Start()
        case 27:Call Pause()
        case 32:Call Disparar()
        case 38:DY=-6
        case 40:DY=6
        case 37:DX=-6
        case 39:DX=6
        End Select   
     End Sub   
     Sub Document_OnKeyUp()
        Select case  Window.Event.KeyCode
        case 38:if DY<>6 then DY=0
        case 40:if DY<>-6 then DY=0
        case 37:if DX<>6 then DX=0
        case 39:if DX<>-6 then DX=0
        End Select
     End Sub  
     
     
